{
  "parser": "@babel/eslint-parser",
  "rules": {
    "no-unused-vars": "off",
    "prettier/prettier": ["error"],
    "@typescript-eslint/ban-ts-comment": "off"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "requireConfigFile": false
  },
  "extends": ["eslint:recommended", "prettier", "plugin:prettier/recommended"],
  "plugins": ["prettier"]
}
